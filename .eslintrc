{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-first-prop-new-line": 0,
    "react/require-render-return": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "new-cap": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "consistent-return": 0,
    "no-else-return": 0,
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "prefer-template": 0,
    "arrow-body-style": 0
  },
  "plugins": [
    "react"
  ]
}

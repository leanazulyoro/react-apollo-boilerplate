{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ],
    "transform-decorators-legacy",
    "transform-class-properties",
    "syntax-async-functions",
    "transform-regenerator",
    "syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "plugins": ["dynamic-import-node"]
    }
  },
  "sourceMaps": "inline",
  "retainLines": true
}

{
  "name": "tn-cover-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "apollo-link-state": "^0.4.1",
    "babel-cli": "^6.26.0",
    "better-npm-run": "^0.1.0",
    "cors": "^2.8.4",
    "graphql": "^0.13.1",
    "graphql-tag": "^2.8.0",
    "image-webpack-loader": "^4.2.0",
    "isomorphic-config": "^0.2.3",
    "lodash": "^4.17.10",
    "newrelic": "^3.1.0",
    "node-fetch": "^2.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "safer-stringify": "^0.0.1",
    "yn": "^2.0.0"
  },
  "scripts": {
    "start:prod": "better-npm-run start:prod",
    "start:dev": "better-npm-run start:dev",
    "clean": "rimraf build",
    "start:trace": "npm run deploy:prod && node --debug build/server.js",
    "deploy:prod": "npm run clean && webpack --mode production --config ./webpack/webpack.server.config.js && webpack --mode production --config ./webpack/webpack.prod.config.js",
    "test": "jest --verbose --coverage",
    "test:ci": "jest --verbose --coverage --maxWorkers=2",
    "test:watch": "npm test -- --watch",
    "lint": "eslint client/**/*.js server/**/*.js --fix"
  },
  "betterScripts": {
    "start:prod": {
      "command": "npm run deploy:prod && node build/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start:dev": {
      "command": "npm run clean && babel-node --inspect=0.0.0.0:9235 src/server/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(scss|css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/enzyme.config.js"
    ]
  },
  "devDependencies": {
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-css-modules-transform": "^1.5.0",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chokidar": "^2.0.2",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "graphql-tools": "^2.22.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.2",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.16.3",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-node-externals": "^1.7.2"
  }
}
